{
  "name": "ssr-cli",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "npm run dev:client & npm run dev:server",
    "dev:server": "nodemon ./server/index.js",
    "dev:client": "cross-env VUE_APP_ENV=client NODE_ENV=development vue-cli-service serve",
    "build": "npm run build:s && npm run build:c",
    "build:server": "cross-env VUE_APP_ENV=client NODE_ENV=production vue-cli-service build --mode --no-clean",
    "build:client": "cross-env VUE_APP_ENV=server NODE_ENV=production vue-cli-service build --mode --no-clean",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "core-js": "^3.8.3",
    "express": "^4.17.3",
    "koa": "^2.13.4",
    "koa-router": "^10.1.1",
    "koa-static": "^5.0.0",
    "lru-cache": "^7.5.1",
    "vue": "2.5.2",
    "vue-router": "^3.5.1",
    "vue-server-renderer": "2.5.2"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/eslint-parser": "^7.12.16",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.32.0",
    "eslint-plugin-vue": "^8.0.3",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "vue-loader": "^13.3.0",
    "vue-template-compiler": "^2.5.2",
    "webpack": "^4.0.0",
    "webpack-node-externals": "^3.0.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "@babel/eslint-parser"
    },
    "rules": {}
  },
  "browserslist": [
    "last 1 version",
    "> 1%",
    "maintained node versions",
    "not dead"
  ]
}
